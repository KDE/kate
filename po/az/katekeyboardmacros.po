#: keyboardmacrosplugincommands.cpp:26
#, kde-format
msgid "Usage: %1 <name>."
msgstr "İstifadə: %1 <ad>"

#: keyboardmacrosplugincommands.cpp:40 keyboardmacrosplugincommands.cpp:55
#: keyboardmacrosplugincommands.cpp:61
#, kde-format
msgid "No keyboard macro named '%1' found."
msgstr "\"%1\" adlı klaviatura makrosu tapılmadı."

#: keyboardmacrosplugincommands.cpp:49
#, kde-format
msgid "Cannot save empty keyboard macro."
msgstr "Boş klaviatura makrosunu saxlamaq mümkün deyil."

#: keyboardmacrosplugincommands.cpp:81
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmsave &lt;name&gt;</code></p><p>Save current keyboard "
"macro as <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>İstifadə: <code>kmsave&lt;ad&gt;</code></p><p>Hazırkı klaviatura "
"makrosunu <code>&lt;ad&gt;</code> kimi saxlayın.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:84
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmload &lt;name&gt;</code></p><p>Load saved keyboard "
"macro <code>&lt;name&gt;</code> as current macro.</p>%1</qt>"
msgstr ""
"<qt><p>İstifadə: <code>kmload &lt;ad&gt;</code></p><p>Saxlanılmış <code>&lt;"
"ad&gt;</code> klaviatura makrosunu cari makros kimi yükləyin.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:88
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmplay &lt;name&gt;</code></p><p>Play saved keyboard "
"macro <code>&lt;name&gt;</code> without loading it.</p>%1</qt>"
msgstr ""
"<qt><p>İstifadə: <code>kmplay &lt;ad&gt;</code></p><p>Saxlanılmış <code>&lt;"
"ad&gt;</code> klaviatura makrosunu onu yükləmədən baçladın.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:92
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmwipe &lt;name&gt;</code></p><p>Wipe saved keyboard "
"macro <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>İstifadə: <code>&lt;kmwipe&gt;ad</code></p><p>Saxlanılmış <code>&lt;"
"ad&gt;</code> klaviatura makrosunu silin.</p>%1</qt>"

#: keyboardmacrospluginview.cpp:33
#, kde-format
msgid "&Keyboard Macros"
msgstr "&Klaviatura makrosları"

#: keyboardmacrospluginview.cpp:36
#, kde-format
msgid "Record and play keyboard macros."
msgstr "Klaviatura makroslarını yazın və başladın."

#: keyboardmacrospluginview.cpp:41 keyboardmacrospluginview.cpp:163
#, kde-format
msgid "&Record Macro..."
msgstr "&Makros yazmaq..."

#: keyboardmacrospluginview.cpp:43
#, kde-format
msgid "Start/stop recording a macro (i.e., keyboard action sequence)."
msgstr ""
"Makrosun yazılmasını başladın/dayandırın (məs., klaviatura əməlləri "
"ardıcıllığı)"

#: keyboardmacrospluginview.cpp:52
#, kde-format
msgid "&Cancel Macro Recording"
msgstr "Makrosun yazılmasını &ləğv edin"

#: keyboardmacrospluginview.cpp:54
#, kde-format
msgid ""
"Cancel ongoing recording (and keep the previous macro as the current one)."
msgstr ""
"Hazırkı yazılmanı ləğv edin (və əvvəlki makrosu cari makros kimi saxlayın)."

#: keyboardmacrospluginview.cpp:64
#, kde-format
msgid "&Play Macro"
msgstr "Makrosu &başladın"

#: keyboardmacrospluginview.cpp:66
#, kde-format
msgid ""
"Play current macro (i.e., execute the last recorded keyboard action "
"sequence)."
msgstr ""
"Cari makrosu başladın (məs., sonuncu yazılmış klaviatura əməlləri "
"ardıcıllığını icra edin)."

#: keyboardmacrospluginview.cpp:76
#, kde-format
msgid "&Save Current Macro"
msgstr "Cari makrosu &saxlayın"

#: keyboardmacrospluginview.cpp:78
#, kde-format
msgid "Give a name to the current macro and persistently save it."
msgstr "Cari makrosa ad verin və onu faylda saxlayın."

#: keyboardmacrospluginview.cpp:90
#, kde-format
msgid "&Load Named Macro..."
msgstr "Adlandırılmış makrosu &yükləyin..."

#: keyboardmacrospluginview.cpp:93
#, kde-format
msgid "Load a named macro as the current one."
msgstr "Adlandırılmış makrosu cari olaraq yükləyin"

#: keyboardmacrospluginview.cpp:98
#, kde-format
msgid "&Play Named Macro..."
msgstr "Adlandırılmış makrosu &başladın..."

#: keyboardmacrospluginview.cpp:101
#, kde-format
msgid "Play a named macro without loading it."
msgstr "Adlandırılmış makrosu onu yükləmədən başladın."

#: keyboardmacrospluginview.cpp:106
#, kde-format
msgid "&Wipe Named Macro..."
msgstr "Adlandırılmış makrosu &silin..."

#: keyboardmacrospluginview.cpp:109
#, kde-format
msgid "Wipe a named macro."
msgstr "Adlandırılmış makrosu silmək."

#: keyboardmacrospluginview.cpp:155
#, kde-format
msgid "End Macro &Recording"
msgstr "Makrosun &yazılmasını sona çatdırın"

