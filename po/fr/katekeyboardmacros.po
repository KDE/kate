#: keyboardmacrosplugin.cpp:66 keyboardmacrosplugin.cpp:102
#, kde-format
msgid "Could not open file '%1'."
msgstr "Impossible d'ouvrir le fichier « %1 »."

#: keyboardmacrosplugin.cpp:72
#, kde-format
msgid "Malformed JSON file '%1': %2"
msgstr "Fichier « JSON » malformé « %1 » : %2"

#: keyboardmacrosplugin.cpp:80
#, kde-format
msgid "Could not load '%1': malformed macro; wiping it."
msgstr "Impossible de charger « %1 » : macro malformée et macro supprimée."

#. i18n: ectx: ToolBar (keyboardmacros)
#: keyboardmacrosplugin.cpp:120 keyboardmacrospluginview.cpp:30
#: keyboardmacrospluginview.cpp:290 keyboardmacrospluginview.cpp:324 ui.rc:12
#, kde-format
msgid "Keyboard Macros"
msgstr "Macros de clavier"

#: keyboardmacrosplugin.cpp:129
#, kde-format
msgid "<b>Keyboard Macros:</b> %1"
msgstr "<b>Macros de clavier : </b> %1"

#: keyboardmacrosplugin.cpp:170
#, kde-format
msgid "Can't save macro during recording. Please end recoding first"
msgstr ""
"Impossible d'enregistrer la macro pendant l'enregistrement. Veuillez d'abord "
"mettre fin au recodage."

#: keyboardmacrosplugin.cpp:241
#, kde-format
msgid "Recording…"
msgstr "Enregistrement..."

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "ended"
msgstr "terminé"

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "canceled"
msgstr "annulé"

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "Recording %1"
msgstr "Enregistrement de %1..."

#: keyboardmacrosplugin.cpp:327
#, kde-format
msgid "Saved '%1'"
msgstr "« %1 » enregistré"

#: keyboardmacrosplugin.cpp:347
#, kde-format
msgid "Loaded '%1'"
msgstr "« %1 » chargé"

#: keyboardmacrosplugin.cpp:365
#, kde-format
msgid "Wiped '%1'"
msgstr "« %1 » supprimé"

#: keyboardmacrosplugincommands.cpp:27
#, kde-format
msgid "Usage: %1 <name>."
msgstr "Utilisation : %1 <name>."

#: keyboardmacrosplugincommands.cpp:41 keyboardmacrosplugincommands.cpp:56
#: keyboardmacrosplugincommands.cpp:62
#, kde-format
msgid "No keyboard macro named '%1' found."
msgstr "Aucune macro de clavier nommée « %1 » trouvée."

#: keyboardmacrosplugincommands.cpp:50
#, kde-format
msgid "Cannot save empty keyboard macro."
msgstr "Impossible d'enregistrer une macro vide de clavier."

#: keyboardmacrosplugincommands.cpp:82
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmsave &lt;name&gt;</code></p><p>Save current keyboard "
"macro as <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>Utilisation : <code>kmsave &lt;name&gt;</code></p><p>Enregistrer la "
"macro courante de clavier comme <code>&lt;name&gt;</code>.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:85
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmload &lt;name&gt;</code></p><p>Load saved keyboard "
"macro <code>&lt;name&gt;</code> as current macro.</p>%1</qt>"
msgstr ""
"<qt><p>Utilisation : <code>kmload &lt;name&gt;</code></p><p>Charger une "
"macro enregistrée de clavier <code>&lt;name&gt;</code> comme macro courante."
"</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:89
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmplay &lt;name&gt;</code></p><p>Play saved keyboard "
"macro <code>&lt;name&gt;</code> without loading it.</p>%1</qt>"
msgstr ""
"<qt><p>Utilisation : <code>kmplay &lt;name&gt;</code></p><p>Exécuter la "
"macro enregistrée de clavier <code>&lt;name&gt;</code> sans la charger.</p>"
"%1</qt>"

#: keyboardmacrosplugincommands.cpp:93
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmwipe &lt;name&gt;</code></p><p>Wipe saved keyboard "
"macro <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>Utilisation : <code>kmwipe &lt;name&gt;</code></p><p>Effacer une "
"macro enregistrée de clavier <code>&lt;name&gt;</code>.</p>%1</qt>"

#: keyboardmacrospluginview.cpp:33
#, kde-format
msgid "&Keyboard Macros"
msgstr "&Macros de clavier"

#: keyboardmacrospluginview.cpp:36
#, kde-format
msgid "Record and play keyboard macros."
msgstr "Enregistrer et lire les macros de clavier."

#: keyboardmacrospluginview.cpp:41 keyboardmacrospluginview.cpp:163
#, kde-format
msgid "&Record Macro..."
msgstr "En&registrer une macro..."

#: keyboardmacrospluginview.cpp:43
#, kde-format
msgid "Start/stop recording a macro (i.e., keyboard action sequence)."
msgstr ""
"Démarrer / Arrêter l'enregistrement d'une macro (Par exemple, une séquence "
"d'action du clavier)"

#: keyboardmacrospluginview.cpp:52
#, kde-format
msgid "&Cancel Macro Recording"
msgstr "Annuler l'enregistrement de ma&cros"

#: keyboardmacrospluginview.cpp:54
#, kde-format
msgid ""
"Cancel ongoing recording (and keep the previous macro as the current one)."
msgstr ""
"Annuler l'enregistrement en cours (et conserver la macro précédente comme "
"celle courante)"

#: keyboardmacrospluginview.cpp:64
#, kde-format
msgid "&Play Macro"
msgstr "Exé&cuter une macro"

#: keyboardmacrospluginview.cpp:66
#, kde-format
msgid ""
"Play current macro (i.e., execute the last recorded keyboard action "
"sequence)."
msgstr ""
"Exécuter la macro courante (Par exemple, exécuter la dernière séquence "
"enregistrée d'actions clavier)."

#: keyboardmacrospluginview.cpp:76
#, kde-format
msgid "&Save Current Macro"
msgstr "Enregi&strer la macro courante"

#: keyboardmacrospluginview.cpp:78
#, kde-format
msgid "Give a name to the current macro and persistently save it."
msgstr ""
"Donner un nom à la macro courante et l'enregistrer de façon permanente."

#: keyboardmacrospluginview.cpp:90
#, kde-format
msgid "&Load Named Macro..."
msgstr "C&harger une macro nommée..."

#: keyboardmacrospluginview.cpp:93
#, kde-format
msgid "Load a named macro as the current one."
msgstr "Charger une macro nommée comme celle courante."

#: keyboardmacrospluginview.cpp:98
#, kde-format
msgid "&Play Named Macro..."
msgstr "Exé&cuter une macro nommée..."

#: keyboardmacrospluginview.cpp:101
#, kde-format
msgid "Play a named macro without loading it."
msgstr "Exécuter une macro nommée sans la charger."

#: keyboardmacrospluginview.cpp:106
#, kde-format
msgid "&Wipe Named Macro..."
msgstr "Suppri&mer une macro nommée..."

#: keyboardmacrospluginview.cpp:109
#, kde-format
msgid "Wipe a named macro."
msgstr "Supprimer une macro nommée."

#: keyboardmacrospluginview.cpp:155
#, kde-format
msgid "End Macro &Recording"
msgstr "Te&rminer l'enregistrement de macro"

#: keyboardmacrospluginview.cpp:180
#, kde-format
msgid "Load %1"
msgstr "Charger %1"

#: keyboardmacrospluginview.cpp:181
#, kde-format
msgid "Load the '%1' macro as the current one."
msgstr "Charger la macro « %1 » comme celle courante."

#: keyboardmacrospluginview.cpp:193
#, kde-format
msgid "Play %1"
msgstr "Lire %1"

#: keyboardmacrospluginview.cpp:194
#, kde-format
msgid "Play the '%1' macro without loading it."
msgstr "Exécuter la macro « %1 » sans la charger."

#: keyboardmacrospluginview.cpp:208
#, kde-format
msgid "Wipe %1"
msgstr "Balayer %1"

#: keyboardmacrospluginview.cpp:209
#, kde-format
msgid "Wipe the '%1' macro."
msgstr "Suppri&mer la macro « %1 »."

#: keyboardmacrospluginview.cpp:290
#, kde-format
msgid "Save current macro as?"
msgstr "Enregistrer la macro courante comme cela ?"

#: keyboardmacrospluginview.cpp:324
#, kde-format
msgid "Wipe the '%1' macro?"
msgstr "Supprimer la macro « %1 » ?"

