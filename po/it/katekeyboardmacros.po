msgid "Malformed JSON file '%1': %2"
msgstr "File JSON «%1» non corretto: %2"

#: keyboardmacrosplugin.cpp:79
#, kde-format
msgid "Could not load '%1': malformed macro; wiping it."
msgstr "Impossibile caricare «%1»: macro non corretta. Pulizia."

