msgstr "&ელემენტის ჩასმა..."

#: plugin_katexmltools.cpp:124
#, kde-format
msgid "&Close Element"
msgstr "&ელემენტის დახურვა"

#: plugin_katexmltools.cpp:128
#, kde-format
msgid "Assign Meta &DTD..."
msgstr "მეტა &DTD-ის მინიჭება..."

#: plugin_katexmltools.cpp:439
#, kde-format
