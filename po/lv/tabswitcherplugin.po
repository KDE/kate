#: tabswitcher.cpp:57
#, kde-format
msgid "Document Switcher"
msgstr "Dokumentu pārslēdzējs"

#: tabswitcher.cpp:102
#, kde-format
msgid "Last Used Views"
msgstr "Pēdējie izmantotie skati"

#: tabswitcher.cpp:105
#, kde-format
msgid "Opens a list to walk through the list of last used views."
msgstr "Atver sarakstu, lai izietu cauri pēdējiem izmantotajiem skatiem."

#: tabswitcher.cpp:106 tabswitcher.cpp:114
#, kde-format
msgid "Walk through the list of last used views"
msgstr "Iet cauri pēdējo izmantoto skatu sarakstam"

#: tabswitcher.cpp:110
#, kde-format
msgid "Last Used Views (Reverse)"
msgstr "Pēdējie izmantotie skati (pretējā secībā)"

#: tabswitcher.cpp:113
#, kde-format
msgid "Opens a list to walk through the list of last used views in reverse."
msgstr ""
"Atver sarakstu, lai pretējā secībā izietu cauri pēdējiem izmantotajiem "
"skatiem."

#: tabswitcher.cpp:118
#, kde-format
msgid "Close View"
msgstr "Aizvērt skatu"

#: tabswitcher.cpp:121 tabswitcher.cpp:122
#, kde-format
msgid "Closes the selected view in the list of last used views."
msgstr "Aizver atlasīto skatu pēdējo izmantoto skatu sarakstā."

