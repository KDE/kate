msgstr "<b>Toetsenbordmacro's:</b> %1"

#: keyboardmacrosplugin.cpp:170
#, kde-format
msgid "Can't save macro during recording. Please end recoding first"
msgstr "Macro opslaan tijdens opnemen kan niet. Beëindig opnemen eerst"

#: keyboardmacrosplugin.cpp:241
#, kde-format
msgid "Recording…"
msgstr "Opnemen…"

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "ended"
msgstr "afgelopen"

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "canceled"
msgstr "geannuleerd"

#: keyboardmacrosplugin.cpp:277
#, kde-format
msgid "Recording %1"
msgstr "%1 wordt opgenomen"

#: keyboardmacrosplugin.cpp:327
#, kde-format
msgid "Saved '%1'"
msgstr "'%1' opgeslagen"

#: keyboardmacrosplugin.cpp:347
#, kde-format
