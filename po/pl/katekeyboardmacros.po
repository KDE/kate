#: keyboardmacrosplugin.cpp:76 keyboardmacrosplugin.cpp:112
#, kde-format
msgid "Could not open file '%1'."
msgstr "Nie można otworzyć pliku '%1'."

#: keyboardmacrosplugin.cpp:82
#, kde-format
msgid "Malformed JSON file '%1': %2"
msgstr "Zniekształcony plik JSON '%1': %2"

#: keyboardmacrosplugin.cpp:90
#, kde-format
msgid "Could not load '%1': malformed macro; wiping it."
msgstr "Nie można wczytać '%1': zniekształcone makro; czyszczenie go."

#: keyboardmacrosplugin.cpp:131 keyboardmacrospluginview.cpp:35
#: keyboardmacrospluginview.cpp:289 keyboardmacrospluginview.cpp:323
#, kde-format
msgid "Keyboard Macros"
msgstr "Makra klawiaturowe"

#: keyboardmacrosplugin.cpp:143
#, kde-format
msgid "<b>Keyboard Macros:</b> %1"
msgstr "<b>Makra klawiaturowe:</b> %1"

#: keyboardmacrosplugin.cpp:242
#, kde-format
msgid "Recording…"
msgstr "Nagrywanie…"

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "ended"
msgstr "zakończone"

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "canceled"
msgstr "anulowane"

#: keyboardmacrosplugin.cpp:278
#, kde-format
msgid "Recording %1"
msgstr "Nagrywanie %1"

#: keyboardmacrosplugin.cpp:321
#, kde-format
msgid "Saved '%1'"
msgstr "Zapisane '%1'"

#: keyboardmacrosplugin.cpp:340
#, kde-format
msgid "Loaded '%1'"
msgstr "Wczytane '%1'"

#: keyboardmacrosplugin.cpp:357
#, kde-format
msgid "Wiped '%1'"
msgstr "Wyczyszczone '%1'"

#: keyboardmacrosplugincommands.cpp:31
#, kde-format
msgid "Usage: %1 <name>."
msgstr "Sposób użycia: %1 <nazwa>."

#: keyboardmacrosplugincommands.cpp:45 keyboardmacrosplugincommands.cpp:60
#: keyboardmacrosplugincommands.cpp:66
#, kde-format
msgid "No keyboard macro named '%1' found."
msgstr "Nie znaleziono żadnego makra o nazwie '%1'."

#: keyboardmacrosplugincommands.cpp:54
#, kde-format
msgid "Cannot save empty keyboard macro."
msgstr "Nie można zapisać pustego makra klawiaturowego."

#: keyboardmacrosplugincommands.cpp:81
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmsave &lt;name&gt;</code></p><p>Save current keyboard "
"macro as <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>Użycie: <code>kmsave &lt;nazwa&gt;</code></p><p>Zapisz bieżące makro "
"klawiaturowe jako <code>&lt;nazwa&gt;</code>.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:84
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmload &lt;name&gt;</code></p><p>Load saved keyboard "
"macro <code>&lt;name&gt;</code> as current macro.</p>%1</qt>"
msgstr ""
"<qt><p>Użycie: <code>kmload &lt;nazwa&gt;</code></p><p>Wczytaj zapisane "
"makro klawiaturowe <code>&lt;nazwa&gt;</code> jako bieżące makro.</p>%1</qt>"

#: keyboardmacrosplugincommands.cpp:88
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmplay &lt;name&gt;</code></p><p>Play saved keyboard "
"macro <code>&lt;name&gt;</code> without loading it.</p>%1</qt>"
msgstr ""
"<qt><p>Użycie: <code>kmplay &lt;nazwa&gt;</code></p><p>Odtwórz zapisane "
"makro klawiaturowe <code>&lt;nazwa&gt;</code> bez jego wczytywania.</p>%1</"
"qt>"

#: keyboardmacrosplugincommands.cpp:92
#, kde-format
msgid ""
"<qt><p>Usage: <code>kmwipe &lt;name&gt;</code></p><p>Wipe saved keyboard "
"macro <code>&lt;name&gt;</code>.</p>%1</qt>"
msgstr ""
"<qt><p>Użycie: <code>kmwipe &lt;nazwa&gt;</code></p><p>Wyczyść zapisane "
"makro klawiaturowe <code>&lt;nazwa&gt;</code>.</p>%1</qt>"

#: keyboardmacrospluginview.cpp:38
#, kde-format
msgid "&Keyboard Macros"
msgstr "Makra &klawiaturowe"

#: keyboardmacrospluginview.cpp:41
#, kde-format
msgid "Record and play keyboard macros."
msgstr "Nagraj i odtwórz makra klawiaturowe."

#: keyboardmacrospluginview.cpp:46 keyboardmacrospluginview.cpp:163
#, kde-format
msgid "&Record Macro..."
msgstr "Nag&raj makro..."

#: keyboardmacrospluginview.cpp:48
#, kde-format
msgid "Start/stop recording a macro (i.e., keyboard action sequence)."
msgstr ""
"Rozpocznij/zatrzymaj nagrywanie makra (tj. naciskanie klawiszy na "
"klawiaturze)."

#: keyboardmacrospluginview.cpp:57
#, kde-format
msgid "&Cancel Macro Recording"
msgstr "Zanie&chaj nagrywania makra"

#: keyboardmacrospluginview.cpp:59
#, kde-format
msgid ""
"Cancel ongoing recording (and keep the previous macro as the current one)."
msgstr ""
"Zaniechaj dalszego nagrywania (i zachowaj poprzednie makro jako bieżące)."

#: keyboardmacrospluginview.cpp:69
#, kde-format
msgid "&Play Macro"
msgstr "&Odtwórz makro"

#: keyboardmacrospluginview.cpp:71
#, kde-format
msgid ""
"Play current macro (i.e., execute the last recorded keyboard action "
"sequence)."
msgstr ""
"Odtwórz bieżące makro (tj. wykonaj ostatnio nagrane naciśnięcia klawiszy na "
"klawiaturze)"

#: keyboardmacrospluginview.cpp:81
#, kde-format
msgid "&Save Current Macro"
msgstr "Zapi&sz bieżące makro"

#: keyboardmacrospluginview.cpp:83
#, kde-format
msgid "Give a name to the current macro and persistently save it."
msgstr "Nadaj nazwę bieżącemu makru i trwale je zapisz."

#: keyboardmacrospluginview.cpp:95
#, kde-format
msgid "&Load Named Macro..."
msgstr "Wczy&taj nazwane makro..."

#: keyboardmacrospluginview.cpp:98
#, kde-format
msgid "Load a named macro as the current one."
msgstr "Wczytaj nazwane makro jako bieżące."

#: keyboardmacrospluginview.cpp:103
#, kde-format
msgid "&Play Named Macro..."
msgstr "&Odtwórz nazwane makro..."

#: keyboardmacrospluginview.cpp:106
#, kde-format
msgid "Play a named macro without loading it."
msgstr "Odtwórz nazwane makro bez jego wczytywania."

#: keyboardmacrospluginview.cpp:111
#, kde-format
msgid "&Wipe Named Macro..."
msgstr "&Wyczyść nazwane makro..."

#: keyboardmacrospluginview.cpp:114
#, kde-format
msgid "Wipe a named macro."
msgstr "Wyczyść nazwane makro."

#: keyboardmacrospluginview.cpp:155
#, kde-format
msgid "End Macro &Recording"
msgstr "Zakończ nagrywanie mak&ra"

#: keyboardmacrospluginview.cpp:183
#, kde-format
msgid "Load the '%1' macro as the current one."
msgstr "Wczytaj makro '%1' jako bieżące."

#: keyboardmacrospluginview.cpp:196
#, kde-format
msgid "Play the '%1' macro without loading it."
msgstr "Odtwórz makro '%1' bez jego wczytywania."

#: keyboardmacrospluginview.cpp:211
#, kde-format
msgid "Wipe the '%1' macro."
msgstr "Wyczyść makro '%1'."

#: keyboardmacrospluginview.cpp:289
#, kde-format
msgid "Save current macro as?"
msgstr "Czy zapisać bieżące makro jako?"

#: keyboardmacrospluginview.cpp:323
#, kde-format
msgid "Wipe the '%1' macro?"
msgstr "Czy wyczyścić makro '%1'?"

#. i18n: ectx: Menu (tools)
#: ui.rc:6
#, kde-format
msgid "&Tools"
msgstr "&Narzędzia"
