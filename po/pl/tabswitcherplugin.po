#: tabswitcher.cpp:57
#, kde-format
msgid "Document Switcher"
msgstr "Przełącznik dokumentów"

#: tabswitcher.cpp:102
#, kde-format
msgid "Last Used Views"
msgstr "Ostatnio używane widoki"

#: tabswitcher.cpp:105
#, kde-format
msgid "Opens a list to walk through the list of last used views."
msgstr "Otwiera listę do przeglądania listy ostatnio używanych widoków."

#: tabswitcher.cpp:106 tabswitcher.cpp:114
#, kde-format
msgid "Walk through the list of last used views"
msgstr "Przejdź przez listę ostatnio używanych widoków"

#: tabswitcher.cpp:110
#, kde-format
msgid "Last Used Views (Reverse)"
msgstr "Ostatnio używane widoki (wstecz)"

#: tabswitcher.cpp:113
#, kde-format
msgid "Opens a list to walk through the list of last used views in reverse."
msgstr ""
"Otwiera listę do przeglądania listy ostatnio używanych widoków w kierunku "
"wstecznym."

#: tabswitcher.cpp:118
#, kde-format
msgid "Close View"
msgstr "Zamknij widok"

#: tabswitcher.cpp:121 tabswitcher.cpp:122
#, kde-format
msgid "Closes the selected view in the list of last used views."
msgstr "Zamyka zaznaczony widok na liście ostatnio używanych widoków."

