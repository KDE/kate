#: tabswitcher.cpp:57
#, kde-format
msgid "Document Switcher"
msgstr "दस्तावेज स्विचर"

#: tabswitcher.cpp:102
#, kde-format
msgid "Last Used Views"
msgstr "अन्तिमप्रयुक्तानि दृश्यानि"

#: tabswitcher.cpp:105
#, kde-format
msgid "Opens a list to walk through the list of last used views."
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं गन्तुं सूचीं उद्घाटयति ।"

#: tabswitcher.cpp:106 tabswitcher.cpp:114
#, kde-format
msgid "Walk through the list of last used views"
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं गच्छन्तु"

#: tabswitcher.cpp:110
#, kde-format
msgid "Last Used Views (Reverse)"
msgstr "अन्तिमप्रयुक्तदृश्यानि (विपरीतानि)"

#: tabswitcher.cpp:113
#, kde-format
msgid "Opens a list to walk through the list of last used views in reverse."
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीं विपरीतरूपेण गन्तुं सूचीं उद्घाटयति ।"

#: tabswitcher.cpp:118
#, kde-format
msgid "Close View"
msgstr "Close View इति"

#: tabswitcher.cpp:121 tabswitcher.cpp:122
#, kde-format
msgid "Closes the selected view in the list of last used views."
msgstr "अन्तिमप्रयुक्तदृश्यानां सूचीयां चयनितं दृश्यं पिधायति ।"

#. i18n: ectx: Menu (go)
#: ui.rc:6
#, kde-format
msgid "&Go"
msgstr "&गच्छ"
