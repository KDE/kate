#: plugin_katexmlcheck.cpp:249
#, kde-format
msgid ""
"<b>Error:</b> Failed to find xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Chyba:</b> Nepodarilo sa nájsť xmllint. Uistite sa, že xmllint je "
"nainštalovaný. Je súčasťou libxml2."

#: plugin_katexmlcheck.cpp:332
#, kde-format
msgid ""
"<b>Error:</b> Failed to execute xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Chyba:</b> Zlyhalo spustenie xmllint. Prosím, uistite sa, že je "
"nainštalovaný. Je to súčasť libxml2."

