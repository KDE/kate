#: plugin_katexmlcheck.cpp:246
#, kde-format
msgid ""
"<b>Error:</b> Failed to find xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Napaka:</b> ni bilo mogoče najti programa xmllint. Prepričajte se, da je "
"xmllint nameščen. Gre za del libxml2."

#: plugin_katexmlcheck.cpp:329
#, kde-format
msgid ""
"<b>Error:</b> Failed to execute xmllint. Please make sure that xmllint is "
"installed. It is part of libxml2."
msgstr ""
"<b>Napaka:</b> ni bilo mogoče izvesti xmllint. Prepričajte se, da je xmllint "
"nameščen. Gre za del libxml2."

