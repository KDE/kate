msgid "&Pipe to Terminal"
msgstr "&Gửi qua dòng lệnh"

#: kateconsole.cpp:148
#, kde-format
msgctxt "@action"
msgid "S&ynchronize Terminal with Current Document"
msgstr "Đồng &bộ hoá dòng lệnh với tài liệu hiện tại"

#: kateconsole.cpp:152
#, kde-format
msgctxt "@action"
msgid "Run Current Document"
msgstr "Chạy tài liệu hiện tại"

#: kateconsole.cpp:157 kateconsole.cpp:506
#, kde-format
msgctxt "@action"
msgid "S&how Terminal Panel"
msgstr "Hiện bảng \"Dòng &lệnh\""

#: kateconsole.cpp:163
