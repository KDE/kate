#: tabswitcher.cpp:57
#, kde-format
msgid "Document Switcher"
msgstr "Trình chuyển tài liệu"

#: tabswitcher.cpp:105
#, kde-format
msgid "Last Used Views"
msgstr "Khung xem dùng gần nhất"

#: tabswitcher.cpp:108
#, kde-format
msgid "Opens a list to walk through the list of last used views."
msgstr "Mở một danh sách để duyệt qua các khung xem được dùng gần đây nhất."

#: tabswitcher.cpp:109 tabswitcher.cpp:117
#, kde-format
msgid "Walk through the list of last used views"
msgstr "Duyệt qua danh sách các khung xem dùng gần nhất"

#: tabswitcher.cpp:113
#, kde-format
msgid "Last Used Views (Reverse)"
msgstr "Khung xem dùng gần nhất (ngược lại)"

#: tabswitcher.cpp:116
#, kde-format
msgid "Opens a list to walk through the list of last used views in reverse."
msgstr ""
"Mở một danh sách để duyệt qua theo chiều ngược lại các khung xem được dùng "
"gần đây nhất."

#: tabswitcher.cpp:121
#, kde-format
msgid "Close View"
msgstr "Đóng khung xem"

#: tabswitcher.cpp:124 tabswitcher.cpp:125
#, kde-format
msgid "Closes the selected view in the list of last used views."
msgstr ""
"Đóng khung xem được chọn trong danh sách các khung xem được dùng gần đây "
"nhất."

